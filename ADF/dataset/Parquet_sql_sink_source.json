{
	"name": "Parquet_sql_sink_source",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage_sink",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@item().filename_sink",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(split(item().filename_sink,'.')[1],/Datepart=',formatDateTime(utcNow(),'yyyy-MM-dd'))",
					"type": "Expression"
				},
				"fileSystem": "raw"
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}