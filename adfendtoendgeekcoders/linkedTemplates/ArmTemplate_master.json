		"AzureDataLakeStorage_sink_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "4d40b69d-9108-433d-95b4-01e4de3630f8"
		},
		"AzureDataLakeStorage_sink_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "02bcd83b-41c7-4070-951e-6ca1397ba34c"
		},
		"AzureDataLakeStorage_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://endtoendgen2source.dfs.core.windows.net/"
